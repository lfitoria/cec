{% extends 'base.html.twig' %}

{% block title %}File{% endblock %}

{% block body %}
    <h1>File</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ file.id }}</td>
            </tr>
            <tr>
                <th>Filename</th>
                <td>{{ file.filename }}</td>
            </tr>
            <tr>
                <th>Original_name</th>
                <td>{{ file.originalName }}</td>
            </tr>
            <tr>
                <th>Mime</th>
                <td>{{ file.mime }}</td>
            </tr>
            <tr>
                <th>Size</th>
                <td>{{ file.size }}</td>
            </tr>
            <tr>
                <th>QuestionCode</th>
                <td>{{ file.questionCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('file_index') }}">back to list</a>

    <a href="{{ path('file_edit', {'id': file.id}) }}">edit</a>

    {{ include('file/_delete_form.html.twig') }}
{% endblock %}
